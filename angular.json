{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "basic-app": {
      "root": "apps/basic-app",
      "sourceRoot": "apps/basic-app/src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/basic-app",
            "index": "apps/basic-app/src/index.html",
            "main": "apps/basic-app/src/main.ts",
            "polyfills": "apps/basic-app/src/polyfills.ts",
            "tsConfig": "apps/basic-app/tsconfig.app.json",
            "assets": [
              "apps/basic-app/src/favicon.ico",
              "apps/basic-app/src/assets"
            ],
            "styles": [
              "apps/basic-app/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/basic-app/src/environments/environment.ts",
                  "with": "apps/basic-app/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "basic-app:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "basic-app:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "basic-app:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/basic-app/src/test.ts",
            "polyfills": "apps/basic-app/src/polyfills.ts",
            "tsConfig": "apps/basic-app/tsconfig.spec.json",
            "karmaConfig": "apps/basic-app/karma.conf.js",
            "styles": [
              "apps/basic-app/src/styles.css"
            ],
            "scripts": [],
            "assets": [
              "apps/basic-app/src/favicon.ico",
              "apps/basic-app/src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/basic-app/tsconfig.app.json",
              "apps/basic-app/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "content-lib": {
      "root": "libs/content-lib",
      "sourceRoot": "libs/content-lib/src",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "libs/content-lib/tsconfig.lib.json",
            "project": "libs/content-lib/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "libs/content-lib/src/test.ts",
            "tsConfig": "libs/content-lib/tsconfig.spec.json",
            "karmaConfig": "libs/content-lib/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/content-lib/tsconfig.lib.json",
              "libs/content-lib/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "basic-app",
  "cli": {
    "defaultCollection": "@nrwl/schematics"
  }
}
